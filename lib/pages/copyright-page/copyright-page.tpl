<div class="copyright-page">
	<p class="copyright-info">{{ copyright_info }}</p>
	<div class="book-info">
		<p>{{ title }} / {{ author }}{{ wrote }};</p>
		<p>{{ publish_by_zine }}{{ comma }}{{ year_date }}</p>
		<p>{{ author_column }}<u>{{ column_link }}</u></p>
	</div>
	<p class="copyright">{{ copyright }}</p>
	<p class="title">{{ title }}</p>
	<p class="author">{{ text_author }}{{ colon }}{{ author }}</p>
	<p class="word-count">{{ text_word_count }}{{ colon }}{{ word_count }}</p>
	<p class="publisher">{{ text_publisher }}{{ colon }}Zine(<u>https://zine.la</u>)</p>
	<img class="logo" src="../../../../../logo.png"></img>
</div>